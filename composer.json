{
    "name": "aura/marshal",
    "version": "1.2.1",
    "type": "aura-package",
    "description": "The Aura Marshal package is a data-object marshalling tool; it takes results from data sources and marshals those result sets into domain model objects of your own design, preserving data relationships along the way.",
    "keywords": [
        "data",
        "data mapper",
        "data marshal",
        "data marshalling",
        "data object"
    ],
    "homepage": "http://auraphp.com/Aura.Marshal",
    "time": "2014-05-31",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura.Marshal Contributors",
            "homepage": "https://github.com/auraphp/Aura.Marshal/contributors"
        },
        {
            "name": "Paul M. Jones",
            "email": "pmjones88@gmail.com",
            "homepage": "http://paul-m-jones.com"
        },
        {
            "name": "Hari KT",
            "email": "kthari85@gmail.com",
            "homepage": "http://harikt.com"
        },
        {
            "name": "Pascal Borrelli",
            "email": "pascal@borreli.com"
        },
        {
            "name": "Nakymaton",
            "email": "nakymaton.hdc@googlemail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "aura/installer-default": "1.0.*"
    },
    "autoload": {
        "psr-0": {
            "Aura\\Marshal": "src/"
        },
        "psr-4": {
            "Aura\\Marshal\\_Config\\": "config/"
        }
    },
    "extra": {
        "aura": {
            "type": "library",
            "config": {
                "common": "Aura\\Marshal\\_Config\\Common"
            }
        },
        "branch-alias": {
            "dev-develop": "1.2.x-dev"
        }
    }
}
